{
	"name": "@ctx-core/security",
	"version": "7.0.14",
	"description": "ctx-core security",
	"keywords": [
		"ctx-core",
		"security"
	],
	"homepage": "https://github.com/ctx-core/security#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/security/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/security/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/object": "workspace:^21.0.18",
		"esm": "^3.2.25"
	},
	"devDependencies": {
		"typescript": "^4.4.3"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}